{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "events"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag",
      "mappedBy": "event"
    },
    "users": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "private": true,
      "inversedBy": "events"
    },
    "date": {
      "type": "datetime"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "price": {
      "type": "integer",
      "required": true,
      "default": 0
    },
    "slug": {
      "type": "customField",
      "options": {
        "pattern": "title"
      },
      "customField": "plugin::slug.slug"
    },
    "location": {
      "type": "dynamiczone",
      "components": [
        "shared.on-site",
        "shared.online-location"
      ],
      "max": 1
    },
    "video": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "schedule": {
      "displayName": "Ã‡izelge",
      "type": "component",
      "repeatable": true,
      "component": "shared.cizelge"
    },
    "content": {
      "type": "blocks"
    },
    "maxCapacity": {
      "type": "integer",
      "required": false,
      "default": -1
    }
  }
}
